res.data ==true ) alert('frauddddddddddddd')\n                else alert('not fraud')\n              })\n             \n\n            \n        } catch (err) {\n            console.error(err);\n           \n        }\n    };\n  \n    return (\n        <div>\n            <h1 className=\"title\">Upload an Image</h1>\n          \n            <form onSubmit={handleSubmitFile} className=\"form\">\n                <input\n                    id=\"fileInput\"\n                    type=\"file\"\n                    name=\"file\"\n                    onChange={handleFileInputChange}\n                    value={fileInputState}\n                    className=\"form-input\"\n                />\n                <button className=\"btn\" type=\"submit\">\n                    Submit\n                </button>\n            </form>\n            {previewSource && (\n                <img\n                    src={previewSource}\n                    alt=\"chosen\"\n                    style={{ height: '300px' }}\n                />\n            )}\n        </div>\n    );\n}\n\nexport default Aadhar;\n","/home/epshita/Desktop/Fraud-Detection/client/src/redux/actions/userAction.js",[],"/home/epshita/Desktop/Fraud-Detection/client/src/screens/HomeScreen.js",["165","166","167"],"import './HomeScreen.css'\nimport {useEffect} from 'react'\nimport {useDispatch, useSelector} from 'react-redux'\nimport axios from 'axios'\n\n// Components\nimport Product from '../components/Product'\n\n//Actions\nimport {getProducts as listProducts} from '../redux/actions/productActions'\nimport {setUserDeatils} from '../redux/actions/userAction'\n\nconst HomeScreen = () => {\n  const dispatch = useDispatch()\n  const url = 'https://google.com'\n\n  const getProducts = useSelector(state => state.getProducts)\n  const {products, loading, error} = getProducts\n\n  useEffect(() => {\n    dispatch(listProducts())\n  }, [dispatch])\n\n  useEffect(() => {\n    dispatch(setUserDeatils())\n  }, [dispatch])\n\n  const ha